  <div class="controls">
    <input type="text" id="searchBar" placeholder="üîç Search products..." />
    <select id="categoryFilter">
      <option value="all">All Categories</option>
      <option value="Electronics">Electronics</option>
      <option value="Fashion">Fashion</option>
      <option value="Home">Home</option>
      <option value="Books">Books</option>
    </select>
    <select id="sortBy">
      <option value="price">Sort by Price</option>
      <option value="name">Sort by Name</option>
    </select>
  </div>

  <div class="products" id="productList"></div>

  <footer>¬© 2025 Bhavsss | Designed for Internship Showcase üöÄ</footer>

  <script>
    const products = [
      { name: "Smartphone X", category: "Electronics", price: 699, image: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRkVgpcyiGkHTqRe_fcjR0GCJAz9e8JpKgjYA&s" },
      { name: "Fashion Shoes", category: "Fashion", price: 89, image: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSuNygR843-JY8JarSPWSQOQ0NxkOqggWfgNw&s" },
      { name: "Coffee Table Book", category: "Books", price: 39, image: "https://m.media-amazon.com/images/I/91OINeHnJGL._AC_UF1000,1000_QL80_.jpg" },
      { name: "LED TV 42\"", category: "Electronics", price: 499, image: "https://m.media-amazon.com/images/I/71vZypjNkPS._AC_UF1000,1000_QL80_.jpg" },
      { name: "Designer Bag", category: "Fashion", price: 199, image: "https://images-cdn.ubuy.co.in/6548d0822a5a215f080a8194-color-block-crossbody-bags-for-women.jpg" },
      { name: "Wall Painting", category: "Home", price: 149, image: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRyso3dDmz09NBVAxmKSU3zqvuicF0iuPKR2g&s" },
      { name: "Wireless Earbuds", category: "Electronics", price: 59, image: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSds0FxQOhC7nJWWQJqxDBhYi8JFxVfR0gQdw&s" },
      { name: "Modern Lamp", category: "Home", price: 89, image: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcS1OhK5Hu8_RJDyU8Fcx7mjumcOWS4mjdD4Tw&s" },
      { name: "Cookbook 2023", category: "Books", price: 55, image: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQ1ZnT0wIWRJyYp9WR-MpmqdoZcNrvlub71tg&s" },
      { name: "Bluetooth Speaker", category: "Electronics", price: 120, image: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSsU9xgSPQ63l8m_Mk0DKkgYR_FnUtLBZL0pg&s" }
    ];

    const productList = document.getElementById("productList");
    const categoryFilter = document.getElementById("categoryFilter");
    const sortBy = document.getElementById("sortBy");
    const searchBar = document.getElementById("searchBar");

    function displayProducts(list) {
      productList.innerHTML = "";
      list.forEach((p, index) => {
        const div = document.createElement("div");
        div.className = "product";
        div.innerHTML = `
          <div class="badge">${p.category}</div>
          <img src="${p.image}" alt="${p.name}" />
          <div class="product-content">
            <h3>${p.name}</h3>
            <p><strong>Price:</strong> ‚Çπ${p.price}</p>
          </div>
          <div class="actions">
            <button class="wishlist-btn" onclick="alert('Added to Wishlist: ${p.name}')">üåü Wishlist</button>
            <button class="cart-btn" onclick="alert('Added to Cart: ${p.name}')">üõí Cart</button>
            <button class="heart" onclick="toggleLike(this)">‚ù§Ô∏è</button>
          </div>
        `;
        productList.appendChild(div);
      });
    }

    function toggleLike(button) {
      button.classList.toggle("liked");
    }

    function filterAndSort() {
      let filtered = [...products];
      const cat = categoryFilter.value;
      const sort = sortBy.value;
      const search = searchBar.value.toLowerCase();

      if (cat !== "all") {
        filtered = filtered.filter(p => p.category === cat);
      }

      if (search) {
        filtered = filtered.filter(p => p.name.toLowerCase().includes(search));
      }

      if (sort === "price") {
        filtered.sort((a, b) => a.price - b.price);
      } else if (sort === "name") {
        filtered.sort((a, b) => a.name.localeCompare(b.name));
      }

      displayProducts(filtered);
    }

    categoryFilter.addEventListener("change", filterAndSort);
    sortBy.addEventListener("change", filterAndSort);
    searchBar.addEventListener("input", filterAndSort);

    displayProducts(products);
  </script>
</body>
</html>
